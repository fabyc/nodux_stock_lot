<data>
    <xpath expr="/tree" position="replace_attributes">
        <tree string="Moves" editable="bottom"/>
    </xpath>

    <xpath expr="/tree/field[@name='lot']" position="after">
      <field name="num_lot"/>
    </xpath>

    <xpath expr="/tree/field[@name='lot']" position="replace">
        <field name="lot" tree_invisible="1"/>
    </xpath>
</data>
